{
	"name": "Musicbox",
	"version": "1.0.0",
	"description": "A music library organizer and player.",
	"homepage": "./",
	"license": "GPL-3.0-only",
	"author": {
		"name": "Jenny Belanger",
		"email": "mail@jennybelanger.com",
		"url": "https://www.jennybelanger.com/"
	},
	"main": "public/electron.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/jlbelanger/musicbox.git"
	},
	"scripts": {
		"start": "npx browserslist@latest --update-db && concurrently \"yarn start:react\" \"wait-on http://127.0.0.1:3000 -l && yarn start:electron\"",
		"start:electron": "node_modules/.bin/electron .",
		"start:react": "react-scripts start",
		"build": "react-scripts build",
		"dist": "electron-builder",
		"lint": "yarn lint:css && yarn lint:js",
		"lint:css": "node_modules/.bin/stylelint 'src/scss/**/*.scss'",
		"lint:js": "node_modules/.bin/eslint . --ext .js",
		"predist": "yarn build",
		"test": "node_modules/.bin/jest --watch",
		"test:cypress": "yarn cypress open"
	},
	"dependencies": {
		"@babel/plugin-transform-runtime": "^7.19.6",
		"@babel/preset-env": "^7.20.2",
		"@reduxjs/toolkit": "^1.9.1",
		"@svgr/webpack": "^6.5.1",
		"dateformat": "^5.0.3",
		"electron-is-dev": "^2.0.0",
		"luxon": "^3.1.1",
		"md5": "^2.3.0",
		"music-metadata": "^7.13.0",
		"normalize.css": "^8.0.1",
		"plist": "^3.0.6",
		"prop-types": "^15.8.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^8.0.5",
		"react-scripts": "^5.0.1",
		"redux-watch": "^1.2.0",
		"sass": "^1.57.1",
		"seedrandom": "^3.0.5",
		"tabulator-tables": "^5.4.3"
	},
	"devDependencies": {
		"@babel/core": "^7.20.7",
		"@babel/eslint-parser": "^7.19.1",
		"@babel/preset-react": "^7.18.6",
		"concurrently": "^7.6.0",
		"cypress": "^12.2.0",
		"electron": "^22.0.0",
		"electron-builder": "^23.6.0",
		"eslint": "^8.30.0",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-plugin-import": "2.25.3",
		"eslint-plugin-jsx-a11y": "^6.6.1",
		"eslint-plugin-react": "^7.31.11",
		"eslint-plugin-react-hooks": "^4.6.0",
		"stylelint": "^14.16.0",
		"stylelint-config-standard-scss": "^6.1.0",
		"stylelint-order": "^5.0.0",
		"stylelint-scss": "^4.3.0",
		"wait-on": "^7.0.1"
	},
	"build": {
		"appId": "com.jennybelanger.musicbox",
		"mac": {
			"category": "public.app-category.music"
		}
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}
