{
	"name": "Musicbox",
	"version": "1.0.0",
	"description": "A music library organizer and player.",
	"author": {
		"name": "Jenny Belanger",
		"email": "mail@jennybelanger.com",
		"url": "https://www.jennybelanger.com/"
	},
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jlbelanger/musicbox.git"
	},
	"main": "public/electron.js",
	"homepage": "./",
	"build": {
		"appId": "com.jennybelanger.musicbox",
		"mac": {
			"category": "public.app-category.music"
		}
	},
	"scripts": {
		"start": "concurrently \"BROWSER=none yarn start:react\" \"wait-on http://localhost:3000 && electron .\"",
		"start:react": "react-scripts start",
		"build": "react-scripts build",
		"dist": "electron-builder",
		"lint": "yarn lint:css && yarn lint:js",
		"lint:css": "node_modules/.bin/stylelint 'src/scss/**/*.scss'",
		"lint:js": "node_modules/.bin/eslint src/js/*",
		"predist": "yarn build",
		"test": "node_modules/.bin/jest --watch",
		"test:cypress": "yarn cypress open"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^4.2.4",
		"@testing-library/react": "^9.3.2",
		"@testing-library/user-event": "^7.1.2",
		"concurrently": "^5.2.0",
		"cypress": "^4.11.0",
		"electron": "^11.0.0",
		"electron-builder": "^22.8.0",
		"eslint": "^6.6.0",
		"eslint-config-airbnb": "^18.0.1",
		"eslint-plugin-import": "^2.18.2",
		"eslint-plugin-jsx-a11y": "^6.2.3",
		"eslint-plugin-react": "^7.17.0",
		"sass-lint": "^1.13.1",
		"stylelint": "^10.1.0",
		"stylelint-config-standard-scss": "^1.1.0",
		"stylelint-order": "^4.1.0",
		"stylelint-scss": "^3.13.0",
		"wait-on": "^5.1.0"
	},
	"dependencies": {
		"@babel/plugin-transform-runtime": "^7.11.0",
		"@babel/preset-env": "^7.11.0",
		"@reduxjs/toolkit": "^1.1.0",
		"@svgr/webpack": "^5.4.0",
		"dateformat": "^3.0.3",
		"electron-is-dev": "^1.2.0",
		"jest": "24.9.0",
		"moment": "^2.27.0",
		"moment-timezone": "^0.5.31",
		"music-metadata": "^7.0.2",
		"node-sass": "^4.14.1",
		"normalize.css": "^8.0.1",
		"plist": "^3.0.1",
		"prop-types": "^15.7.2",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"react-redux": "^7.1.3",
		"react-scripts": "3.4.1",
		"redux-watch": "^1.1.1",
		"seedrandom": "^3.0.5",
		"tabulator-tables": "^4.7.2"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
