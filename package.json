{
	"name": "Musicbox",
	"version": "1.0.0",
	"description": "A music library organizer and player.",
	"homepage": "./",
	"license": "GPL-3.0-only",
	"author": {
		"name": "Jenny Belanger",
		"email": "mail@jennybelanger.com",
		"url": "https://www.jennybelanger.com/"
	},
	"main": "public/electron.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/jlbelanger/musicbox.git"
	},
	"scripts": {
		"start": "concurrently \"ESLINT_NO_DEV_ERRORS=true BROWSER=none yarn start:react\" \"wait-on http://localhost:3000 && electron .\"",
		"start:react": "react-scripts start",
		"build": "react-scripts build",
		"dist": "electron-builder",
		"lint": "yarn lint:css && yarn lint:js",
		"lint:css": "node_modules/.bin/stylelint 'src/scss/**/*.scss'",
		"lint:js": "node_modules/.bin/eslint . --ext .js",
		"predist": "yarn build",
		"test": "node_modules/.bin/jest --watch",
		"test:cypress": "yarn cypress open"
	},
	"dependencies": {
		"@babel/plugin-transform-runtime": "^7.16.5",
		"@babel/preset-env": "^7.16.5",
		"@reduxjs/toolkit": "^1.7.1",
		"@svgr/webpack": "^6.1.2",
		"dateformat": "^5.0.2",
		"electron-is-dev": "^2.0.0",
		"luxon": "^2.2.0",
		"md5": "^2.3.0",
		"music-metadata": "^7.11.4",
		"sass": "^1.55.0",
		"normalize.css": "^8.0.1",
		"plist": "^3.0.5",
		"prop-types": "^15.8.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-redux": "^7.2.6",
		"react-scripts": "^5.0.0",
		"redux-watch": "^1.2.0",
		"seedrandom": "^3.0.5",
		"tabulator-tables": "^5.0.8"
	},
	"devDependencies": {
		"@babel/core": "^7.16.5",
		"@babel/eslint-parser": "^7.16.5",
		"@babel/preset-react": "^7.16.5",
		"@testing-library/jest-dom": "^5.16.1",
		"@testing-library/react": "^12.1.2",
		"@testing-library/user-event": "^13.5.0",
		"concurrently": "^6.5.1",
		"cypress": "^12.2.0",
		"electron": "^16.2.6",
		"electron-builder": "^23.0.2",
		"eslint": "^8.5.0",
		"eslint-config-airbnb": "^19.0.2",
		"eslint-plugin-import": "^2.25.3",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-react": "^7.28.0",
		"eslint-plugin-react-hooks": "^4.3.0",
		"stylelint": "^14.2.0",
		"stylelint-config-standard-scss": "^3.0.0",
		"stylelint-order": "^5.0.0",
		"stylelint-scss": "^4.1.0",
		"wait-on": "^6.0.0"
	},
	"build": {
		"appId": "com.jennybelanger.musicbox",
		"mac": {
			"category": "public.app-category.music"
		}
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	]
}
